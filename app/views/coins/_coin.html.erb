<div id="<%= dom_id coin %>">
  <p>
    <strong>Description:</strong>
    <%= coin.description %>
  </p>

  <p>
    <strong>Acronym:</strong>
    <%= coin.acronym %>
  </p>

  <p>
    <strong>Url image:</strong>
    <p><%= image_tag coin.url_image, width:25, height:25 %></p>
    <%= coin.url_image %>
    
  </p>

</div>
